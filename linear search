class HelloWorld {
    
    public static void main(String[] args) {
        int a[] = {5,2,6,23,7,27,8,4};
        int n = 7;
        for(int i = 0; i<a.length ;i++)
        {
            if(a[i] == n)
            {
                System.out.println("search found ");
            }
        }
    }
}
